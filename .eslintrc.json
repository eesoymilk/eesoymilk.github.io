{
  "extends": ["plugin:astro/recommended", "prettier"],
  "overrides": [
    {
      "files": ["*.astro"],
      "parser": "astro-eslint-parser",
      "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "extraFileExtensions": [".astro"]
      },
      "rules": {}
    },
    {
      "files": ["*.svelte"],
      "parser": "svelte-eslint-parser"
      "parserOptions": {
        "parser": "@typescript-eslint/parser",
      },
    }
  ]
}
